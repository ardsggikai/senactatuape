---

11-03-2022

SP
enable 
 configure terminal
 hostname SP
 interface fast 0/0
 description porta lan do SP
  no shutdown
  ip add 192.168.100.1 255.255.255.0
  exit
 interface s0/0
 description Porta WAN SP
 no shutdown
 ip add 20.0.0.1 255.0.0.0
 clock rate 128000 
 bandwidth 128
 exit
router rip
 net 20.0.0.0
 net 192.168.100.0
 exit
 exit
copy run star

RJ
enable 
 configure terminal
 hostname RJ
 interface fast 0/0
 description porta lan do rj
  no shutdown
  ip add 192.168.200.1 255.255.255.0
  exit
 interface s0/1
 description Porta WAN que liga RJ em SP
 no shutdown
 ip add 20.0.0.2 255.0.0.0
 (colocar ''clock rate 128000'' em SP ''DCE'')
 bandwidth 128
 exit
router rip
 net 20.0.0.0
 net 192.168.200.0
 exit
 exit
copy run star

---

14-03-2022

RJ (DCE) para BH (DTE)
enable 
 configure terminal
 hostname RJ
 interface f0/0
 description porta lan do rj
 ip add 192.168.200.1 255.255.255.0
 interface s0/0
 description Porta WAN RJ
 no shutdown
 ip add 30.0.0.1 255.0.0.0
 (colocar ''clock rate 250000'' em RJ ''DCE'')
 bandwidth 256
 exit
router rip
 net 30.0.0.0
 net 192.168.210.0
 exit
 exit
copy run star

BH (DTE)
enable 
 configure terminal
 hostname BH
 interface f0/0
 description porta lan do BH
 ip add 192.168.210.1 255.255.255.0
 interface s0/1
 description Porta WAN que liga RJ na BH
 no shutdown
 ip add 30.0.0.2 255.0.0.0
 (colocar ''clock rate 250000'' em RJ ''DCE'')
 bandwidth 256
 exit
 router rip
 net 30.0.0.0
 net 192.168.210.0
 exit
 exit
copy run star

